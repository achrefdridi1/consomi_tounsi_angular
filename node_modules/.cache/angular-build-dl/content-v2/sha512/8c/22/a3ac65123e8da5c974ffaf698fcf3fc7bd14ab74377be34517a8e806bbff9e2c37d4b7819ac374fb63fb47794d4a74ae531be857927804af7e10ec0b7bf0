class UserForumService {
    constructor(http) {
        this.http = http;
    }
    doRegistration(publication, userId) {
        console.log(userId);
        return this.http.post("http://localhost:8081/SpringMVC/publication/add-publication/" + userId, publication, { responseType: 'text' });
    }
    getPublications() {
        return this.http.get("http://localhost:8081/SpringMVC/publication/retrieve-all-publications");
    }
    getPublicationById(id) {
        return this.http.get("http://localhost:9090//findPublication/" + id);
    }
    deletePublication(id) {
        return this.http.delete("http://localhost:9090/cancel/" + id);
    }
    deleteCommentaire(id) {
        return this.http.delete("http://localhost:8081/SpringMVC/commentaire/remove-commentaire/" + id);
    }
    getCommentaires(publicationId) {
        return this.http.get("http://localhost:8081/SpringMVC/commentaire/" + publicationId);
    }
    addCommentaire(commentaire, pubId, userId) {
        return this.http.post("http://localhost:8081/SpringMVC/commentaire/add-commentaire/" + pubId + "/" + userId, commentaire, { responseType: 'text' });
    }
}
